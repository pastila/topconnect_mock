{% extends '@SonataAdmin/CRUD/base_list_field.html.twig' %}
{% block field %}
    {% if field_description.name == 'comment' %}
        {{ settingDescription(object.name) }}
    {% elseif field_description.name == 'value' %}
        {{ settingString(object.name) }}
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}